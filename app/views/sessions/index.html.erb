<p style="color: green"><%= notice %></p>

<h1 class="text-4xl font-bold mb-6">Sessions</h1>

<div id="sessions">
  <% @sessions.each do |session| %>
    <%= render session %>
    <p>
      <%= link_to "Voir", session, class:'btn btn-sm mb-6' %>
    </p>
  <% end %>
</div>

<%= link_to "Nouvelle Session", new_session_path, class:'btn btn-success btn-sm' %>
